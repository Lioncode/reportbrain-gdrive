<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_notification" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="social_default_image_setting" position="after">
                <div class="col-12 col-lg-12 o_setting_box">
                    <div class="o_setting_left_pane"/>
                    <div class="o_setting_right_pane">
                        <label for="notification_task_fetching_interval"/>
                        <field name="notification_task_fetching_interval"/> Miliseconds
                    </div>
                </div>
            </div>
        </field>
    </record>

</odoo>
